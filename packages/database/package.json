{
  "name": "@plexica/database",
  "version": "0.1.0",
  "private": true,
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "scripts": {
    "db:generate": "npx --yes prisma@7.2.0 generate --config ./prisma/prisma.config.ts",
    "db:migrate": "npx prisma migrate dev --config ./prisma/prisma.config.ts",
    "db:migrate:deploy": "npx prisma migrate deploy --config ./prisma/prisma.config.ts",
    "db:migrate:status": "npx prisma migrate status --config ./prisma/prisma.config.ts",
    "db:studio": "npx prisma studio --config ./prisma/prisma.config.ts",
    "db:push": "npx prisma db push --config ./prisma/prisma.config.ts",
    "db:seed": "tsx prisma/seed.ts",
    "db:seed:quickstart": "tsx prisma/seed.quickstart.ts",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@prisma/adapter-pg": "^7.2.0",
    "@prisma/client": "7.2.0",
    "@prisma/driver-adapter-utils": "^7.2.0",
    "dotenv": "^17.2.3",
    "pg": "^8.17.2"
  },
  "devDependencies": {
    "@types/pg": "^8.16.0",
    "prisma": "^7.2.0",
    "tsx": "^4.21.0",
    "vitest": "^4.0.18"
  }
}
