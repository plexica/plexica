{
  "name": "plexica",
  "version": "0.1.0",
  "private": true,
  "description": "Plexica - Cloud-native multi-tenant platform with plugin architecture",
  "packageManager": "pnpm@10.28.1",
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=8.0.0"
  },
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "test": "turbo run test",
    "test:unit": "turbo run test:unit",
    "test:integration": "turbo run test:integration",
    "test:e2e": "turbo run test:e2e",
    "test:coverage": "turbo run test:coverage",
    "lint": "turbo run lint",
    "type-check": "tsc --noEmit",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "clean": "turbo run clean && rm -rf node_modules",
    "db:generate": "pnpm --filter @plexica/database db:generate",
    "db:migrate": "pnpm --filter @plexica/database db:migrate",
    "db:migrate:deploy": "pnpm --filter @plexica/database db:migrate:deploy",
    "db:seed": "pnpm --filter @plexica/database db:seed",
    "db:seed:test": "pnpm --filter @plexica/database db:seed || echo 'Test seed not configured'",
    "db:studio": "pnpm --filter @plexica/database db:studio",
    "infra:start": "./scripts/infra.sh start",
    "infra:stop": "./scripts/infra.sh stop",
    "infra:restart": "./scripts/infra.sh restart",
    "infra:status": "./scripts/infra.sh status",
    "infra:logs": "./scripts/infra.sh logs",
    "infra:clean": "./scripts/infra.sh clean",
    "init:env": "./scripts/init-env.sh",
    "init": "./scripts/init-env.sh"
  },
  "devDependencies": {
    "@types/node": "^25.2.3",
    "prettier": "^3.8.1",
    "turbo": "^2.8.10",
    "typescript": "^5.9.3"
  },
  "pnpm": {
    "overrides": {
      "react": "^19.2.3",
      "react-dom": "^19.2.3",
      "@types/react": "^19.2.9",
      "@types/react-dom": "^19.2.3",
      "hono": ">=4.11.10",
      "lodash": ">=4.17.23",
      "@isaacs/brace-expansion": ">=5.0.1",
      "fast-xml-parser": ">=5.3.6",
      "minimatch": ">=10.2.1",
      "ajv": ">=8.18.0",
      "@eslint/eslintrc>ajv": "^6.12.6",
      "eslint>ajv": "^6.12.6",
      "axios": ">=1.13.5",
      "esbuild": ">=0.25.0"
    },
    "ignoredBuiltDependencies": [
      "@prisma/engines",
      "esbuild",
      "msw",
      "prisma"
    ]
  },
  "dependencies": {
    "jwt-decode": "^4.0.0",
    "kafkajs": "^2.2.4",
    "lru-cache": "^11.2.6",
    "zod": "^4.3.6"
  }
}
